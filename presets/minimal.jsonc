{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "none"
  },
  "display": {
    "separator": " ➜  ",
    "color": {
      "output": "white"
    }
  },
  "modules": [
    "break",
    "break",
    {
      "type": "title",
      "format": " {6}{7}{8}",
      "color": {
        "user": "32",
        "at": "37",
        "host": "35"
      }
    },
    {
      "type": "os",
      "key": " 󰻀 OS ",
      "keyColor": "31"
    },
    {
      "type": "kernel",
      "key": " 󰒄 KER",
      "keyColor": "32"
    },
    {
      "type": "packages",
      "key": " 󰏖 PKG",
      "keyColor": "33"
    },
    {
      "type": "shell",
      "key": " 󱆃 SH ",
      "keyColor": "34"
    },
    {
      "type": "terminal",
      "key": " 󰆍 TER",
      "keyColor": "35"
    },
    {
      "type": "wm",
      "key": " 󱂬 WM ",
      "keyColor": "36"
    },
    {
      "type": "colors",
      "symbol": "square",
      "paddingLeft": 1,
      "block": {
        "range": [1, 6]
      }
    },
    "break"
  ]
}