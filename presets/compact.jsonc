{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "builtin",
    "padding": {
      "top": 0,
      "right": 2
    }
  },
  "display": {
    "separator": " │ ",
    "color": "auto"
  },
  "modules": [
    "title",
    "break",
    {
      "type": "os",
      "key": "",
      "format": "{1}"
    },
    {
      "type": "kernel",
      "key": "",
      "format": "{1}"
    },
    {
      "type": "shell",
      "key": "",
      "format": "{1}"
    },
    "break",
    {
      "type": "host",
      "key": "󰌢",
      "format": "{1}"
    },
    {
      "type": "cpu",
      "key": "󰻠",
      "format": "{2}"
    },
    {
      "type": "memory",
      "key": "󰾆",
      "format": "{2}"
    },
    {
      "type": "disk",
      "key": "󰋊",
      "format": "{2}",
      "showType": true
    },
    "break",
    {
      "type": "uptime",
      "key": "󰅐",
      "format": "{1}"
    },
    {
      "type": "packages",
      "key": "󰏖",
      "format": "{2}"
    },
    {
      "type": "wm",
      "key": "",
      "format": "{1}"
    }
  ]
}